    "snyk": "1.69.11",
    "stampit": "4.1.1",
    "traverson": "6.0.3",
    "traverson-hal": "6.0.0",
    "vision": "4.1.1",
    "visionary": "6.0.2",
    "xhr": "2.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
