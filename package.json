    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-config-utils": "2.3.1",
    "webpack-dev-server": "3.8.0",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "@packtracker/webpack-plugin": "2.2.0",
