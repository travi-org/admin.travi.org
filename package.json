{
  "name": "travi.org-admin",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/travi.org-admin"
  },
  "scripts": {
    "clean": "rimraf dist/ resources/js/ resources/css/ webpack-assets.json tmp/ pacts/",
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "disabled-lint:travis": "travis-lint .travis.yml",
    "lint:engine": "check-engine",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit",
    "pretest:integration": "run-s build",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @wip\"'",
    "test:integration:base": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --tags @wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --tags @focus'",
    "test:pact": "babel-node scripts/pact.js",
    "test": "npm-run-all --print-label clean --parallel lint:* --parallel test:*",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:client": "webpack --env production",
    "build:server": "NODE_ENV=production babel src/server --source-maps --out-dir dist/server",
    "build:shared": "NODE_ENV=production babel src/shared --source-maps --out-dir dist/shared",
    "build": "run-s clean && run-p --print-label build:*",
    "start": "node dist/server/app.js",
    "serve:dev": "nodemon src/server/app.js",
    "serve:dev-local": "nodemon src/server/app.js local",
    "serve:webpack": "babel-node ./webpack-dev-server.js",
    "dev": "NODE_ENV=development run-p serve:webpack serve:dev",
    "dev-local": "NODE_ENV=development run-p serve:webpack serve:dev-local",
    "pact:can-i-deploy": "pact-broker can-i-deploy --pacticipant travi.org-admin --version $GITHUB_SHA --broker-base-url pact-api.travi.org",
    "pact:publish": "pact-broker publish ./pacts/ --consumer-app-version $GITHUB_SHA --broker-base-url https://pact-api.travi.org/ --broker-username $PACT_BROKER_USER --broker-password $PACT_BROKER_PASSWORD",
    "packtracker:publish": "packtracker-upload --stats=resources/js/stats.json",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/node": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.8",
    "@babel/register": "7.13.8",
    "@pact-foundation/pact": "9.15.2",
    "@pact-foundation/pact-node": "10.11.11",
    "@travi/any": "2.0.16",
    "@travi/babel-preset": "3.0.45",
    "@travi/eslint-config": "1.0.62",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-hal": "1.0.0",
    "@travi/eslint-config-mocha": "1.0.9",
    "@travi/eslint-config-react": "1.0.53",
    "assets-webpack-plugin": "7.0.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
