{
  "name": "travi.org-admin",
  "main": "index.js",
  "author": "Matt Travi",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/travi.org-admin"
  },
  "scripts": {
    "clean": "rimraf dist/ resources/js/ resources/css/ webpack-assets.json tmp/",
    "lint:js": "eslint .",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "tests:unit": "BABEL_ENV=node mocha --recursive test/unit",
    "tests:integration": "BABEL_ENV=node cucumber.js test/integration --compiler js:babel-register --tags 'not @wip'",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "BABEL_ENV=node cucumber.js test/integration --compiler js:babel-register --tags @wip",
    "tests:pact": "BABEL_ENV=node babel-node scripts/pact.js",
    "test": "run-s lint:* coverage tests:integration tests:pact",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:client": "BABEL_ENV=node webpack --env production",
    "build:server": "NODE_ENV=production BABEL_ENV=node babel lib/server --out-dir dist/server",
    "build:shared": "NODE_ENV=production BABEL_ENV=node babel lib/shared --out-dir dist/shared",
    "build": "run-s clean && run-p build:*",
    "start": "node dist/server/app.js",
    "postinstall": "run-s build",
    "serve:dev": "nodemon lib/server/app.js",
    "serve:dev-local": "nodemon lib/server/app.js local",
    "serve:webpack": "BABEL_ENV=node babel-node ./webpack-dev-server.js",
    "dev": "NODE_ENV=development run-p serve:webpack serve:dev",
    "dev-local": "NODE_ENV=development run-p serve:webpack serve:dev-local",
    "pact-publish": "BABEL_ENV=node babel-node scripts/publish-pacts-to-broker.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "engines": {
    "node": "7.3.x",
    "npm": "3.10.x"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "4.8.0",
    "@travi/any": "1.0.4",
    "@travi/eslint-config-travi": "1.0.0-beta.3",
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.23.0",
    "balanced-match": "0.4.2",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-immutable": "1.6.0",
    "chai-subset": "1.4.0",
    "cheerio": "0.22.0",
    "clean-webpack-plugin": "0.1.15",
    "concat-map": "0.0.1",
    "coveralls": "2.11.16",
    "css-loader": "0.26.2",
    "cucumber": "2.0.0-rc.7",
    "cz-conventional-changelog": "2.0.0",
    "debug": "2.6.1",
    "dotenv": "4.0.0",
    "dotenv-safe": "4.0.3",
    "enzyme": "2.7.1",
    "extract-text-webpack-plugin": "2.0.0",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "h2o2": "5.4.0",
    "husky": "0.13.1",
    "jsdom": "9.11.0",
    "markdownlint-cli": "0.2.0",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "nock": "9.0.7",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "nyc": "10.1.2",
    "pact": "2.2.0",
    "proxyquire": "1.7.11",
    "react-addons-test-utils": "15.4.2",
    "redux-devtools": "3.3.2",
    "redux-devtools-log-monitor": "1.2.0",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "sinon": "1.17.7",
    "sinon-as-promised": "4.0.2",
    "skin-deep": "1.0.0-alpha2",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "validate-commit-msg": "2.11.1",
