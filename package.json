    "async": "3.1.0",
    "blankie": "4.1.0",
    "boom": "6.0.0",
    "css-modules-require-hook": "4.2.3",
    "glue": "6.0.0",
    "good": "8.1.2",
    "good-console": "8.0.0",
    "good-squeeze": "5.1.0",
    "h2o2": "8.2.0",
    "hapi-mustache": "0.0.1",
    "http-status-codes": "1.3.2",
    "immutable": "4.0.0-rc.1",
    "inert": "5.1.3",
    "lodash": "4.17.15",
    "material-ui": "0.20.2",
