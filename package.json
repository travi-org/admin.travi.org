    "style-loader": "0.20.3",
    "uglify-js": "3.3.14",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-config-utils": "2.3.0",
    "webpack-dev-server": "2.11.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "@travi/admin.travi.org-components": "1.0.24",
    "@travi/hapi-html-request-router": "1.2.0",
    "@travi/hapi-react-router": "1.2.2",
    "@travi/ioc": "1.0.0",
    "@travi/redux-fetch-middleware": "1.1.0",
    "async": "2.6.0",
    "blankie": "3.0.1",
    "boom": "6.0.0",
    "css-modules-require-hook": "4.2.3",
    "glue": "4.2.1",
    "good": "7.3.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.0.2",
    "h2o2": "6.0.2",
    "hapi-mustache": "0.0.1",
    "http-status-codes": "1.3.0",
    "immutable": "4.0.0-rc.1",
    "inert": "4.2.1",
    "lodash": "4.17.5",
    "material-ui": "0.20.0",
    "mz": "2.7.0",
    "negotiator": "0.6.1",
    "newrelic": "2.7.1",
    "postcss-scss": "1.0.4",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-ga": "2.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "3.1.3",
    "react-redux": "5.0.7",
    "react-router": "3.2.0",
    "redial": "0.5.0",
    "redux": "3.7.2",
