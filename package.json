{
  "name": "travi.org-admin",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/travi.org-admin"
  },
  "scripts": {
    "clean": "rimraf dist/ resources/js/ resources/css/ webpack-assets.json tmp/",
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:travis": "travis-lint .travis.yml",
    "tests:unit": "mocha --recursive test/unit",
    "pretests:integration": "run-s build",
    "tests:integration": "run-s 'tests:integration:base -- --tags \"not @wip\"'",
    "tests:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "run-s 'tests:integration:base -- --tags @wip'",
    "tests:pact": "babel-node scripts/pact.js",
    "test": "run-s clean lint:* coverage tests:integration tests:pact",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:client": "webpack --env production",
    "build:server": "NODE_ENV=production babel src/server --source-maps --out-dir dist/server",
    "build:shared": "NODE_ENV=production babel src/shared --source-maps --out-dir dist/shared",
    "build": "run-s clean && run-p --print-label build:*",
    "start": "node dist/server/app.js",
    "serve:dev": "nodemon src/server/app.js",
    "serve:dev-local": "nodemon src/server/app.js local",
    "serve:webpack": "babel-node ./webpack-dev-server.js",
    "dev": "NODE_ENV=development run-p serve:webpack serve:dev",
    "dev-local": "NODE_ENV=development run-p serve:webpack serve:dev-local",
    "pact:publish": "pact publish --pact-files-or-dirs $TRAVIS_BUILD_DIR/pacts/ --consumer-version $TRAVIS_COMMIT --pact-broker https://pact-api.travi.org/ --pact-broker-username $PACT_BROKER_USER --pact-broker-password $PACT_BROKER_PASSWORD",
    "packtracker:publish": "packtracker-upload --stats=resources/js/stats.json",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "commitmsg": "commitlint -e",
    "precommit": "npm test"
  },
  "engines": {
    "node": "10.7.x",
    "npm": "6.1.x"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "9.0.2",
    "@travi/any": "2.0.3",
    "@travi/eslint-config-travi": "1.8.1",
    "assets-webpack-plugin": "3.9.10",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-travi": "1.4.2",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-immutable": "2.1.0",
    "chai-subset": "1.6.0",
    "cheerio": "1.0.0-rc.3",
    "clean-webpack-plugin": "1.0.1",
    "codecov": "3.5.0",
    "commitlint-config-travi": "1.3.3",
    "css-loader": "1.0.0",
    "cucumber": "5.1.0",
    "cz-conventional-changelog": "3.0.0",
    "debug": "4.1.1",
    "dotenv": "8.1.0",
    "dotenv-safe": "8.1.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "extract-text-webpack-plugin": "3.0.2",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "husky": "3.0.4",
    "jsdom": "15.1.1",
    "markdownlint-cli": "0.18.0",
    "mocha": "5.2.0",
    "mocha-junit-reporter": "1.23.1",
    "nock": "10.0.6",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "pact": "4.3.2",
    "proxyquire": "2.1.3",
