    "@hapi/h2o2": "9.0.2",
    "@hapi/inert": "6.0.3",
    "@hapi/scooter": "6.0.1",
    "@hapi/vision": "6.0.1",
    "@packtracker/webpack-plugin": "2.3.0",
    "@sentry/node": "6.2.0",
