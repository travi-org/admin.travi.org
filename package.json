    "webpack-config-utils": "2.3.0",
    "webpack-dev-server": "2.4.1",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "@travi/admin.travi.org-components": "0.11.2",
    "@travi/hapi-html-request-router": "1.1.0",
    "@travi/hapi-react-router": "1.1.3",
    "@travi/ioc": "1.0.0",
    "@travi/redux-fetch-middleware": "1.0.3",
    "async": "2.1.5",
    "blankie": "2.0.0",
    "boom": "4.2.0",
    "glue": "4.1.0",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "hapi-mustache": "0.0.1",
    "history": "4.5.1",
    "http-status-codes": "1.1.6",
    "immutable": "3.8.1",
    "inert": "4.1.0",
    "lodash": "4.17.4",
    "material-ui": "0.17.0",
    "negotiator": "0.6.1",
    "newrelic": "1.37.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-ga": "2.1.2",
    "react-helmet": "4.0.0",
    "react-hot-loader": "3.0.0-beta.5",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "redial": "0.5.0",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-immutable": "3.1.0",
    "scooter": "4.0.0",
    "serialize-javascript": "1.3.0",
    "snyk": "1.25.1",
    "stampit": "3.1.2",
