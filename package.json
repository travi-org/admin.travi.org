{
  "name": "travi.org-admin",
  "main": "index.js",
  "license": "MIT",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/travi/travi.org-admin"
  },
  "scripts": {
    "clean": "rimraf dist/ resources/js/ resources/css/ webpack-assets.json tmp/",
    "lint:js": "eslint . --cache",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:travis": "travis lint --no-interactive",
    "tests:unit": "mocha --recursive test/unit",
    "tests:integration": "run-s 'tests:integration:base -- --tags \"not @wip\"'",
    "tests:integration:base": "cucumber-js test/integration --require-module babel-register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "tests:integration:debug": "DEBUG=test run-s tests:integration",
    "tests:integration:wip": "run-s 'tests:integration:base -- --tags @wip'",
    "tests:pact": "babel-node scripts/pact.js",
    "test": "run-s lint:* coverage tests:integration tests:pact",
    "coverage": "nyc run-s tests:unit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:client": "webpack --env production",
    "build:server": "NODE_ENV=production babel src/server --source-maps --out-dir dist/server",
    "build:shared": "NODE_ENV=production babel src/shared --source-maps --out-dir dist/shared",
    "build": "run-s clean && run-p build:*",
    "start": "node dist/server/app.js",
    "postinstall": "run-s build",
    "serve:dev": "nodemon src/server/app.js",
    "serve:dev-local": "nodemon src/server/app.js local",
    "serve:webpack": "babel-node ./webpack-dev-server.js",
    "dev": "NODE_ENV=development run-p serve:webpack serve:dev",
    "dev-local": "NODE_ENV=development run-p serve:webpack serve:dev-local",
    "pact:publish": "babel-node scripts/publish-pacts-to-broker.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "engines": {
    "node": "8.9.x",
    "npm": "5.6.x"
  },
  "devDependencies": {
    "@pact-foundation/pact-node": "6.11.0",
    "@travi/any": "1.7.2",
    "@travi/eslint-config-travi": "1.6.10",
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-node": "6.1.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-immutable": "1.6.0",
    "chai-subset": "1.6.0",
    "cheerio": "1.0.0-rc.2",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.0.0",
    "commitlint-config-travi": "1.2.29",
    "css-loader": "0.28.11",
    "cucumber": "4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "debug": "3.1.0",
    "dotenv": "5.0.1",
    "dotenv-safe": "4.0.4",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "formatio": "1.2.0",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.14.0",
    "husky": "0.15.0-rc.8",
    "jsdom": "11.6.2",
    "markdownlint-cli": "0.7.1",
    "mocha": "5.0.4",
    "mocha-junit-reporter": "1.17.0",
    "nock": "9.2.3",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "pact": "4.3.2",
    "proxyquire": "1.8.0",
    "react-test-renderer": "16.2.0",
    "referee": "1.2.0",
    "referee-sinon": "1.0.3",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "sinon": "4.4.2",
